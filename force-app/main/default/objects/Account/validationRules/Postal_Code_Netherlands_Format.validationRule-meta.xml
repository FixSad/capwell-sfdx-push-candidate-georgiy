<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Postal_Code_Netherlands_Format</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(ShippingPostalCode)),NOT(REGEX( ShippingPostalCode, &quot;[0-9]{4}\\s[A-Z]{2}&quot;)))</errorConditionFormula>
    <errorDisplayField>ShippingPostalCode</errorDisplayField>
    <errorMessage>Shipping Post code format is 1234 AB</errorMessage>
</ValidationRule>
